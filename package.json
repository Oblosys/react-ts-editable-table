{
  "name": "react-use-table-editor",
  "author": "Martijn Schrage (https://www.oblomov.com)",
  "version": "0.1.0",
  "description": "React hook that provides dirty-tracking on table rows and cells",
  "main": "./dist/useTableEditor.js",
  "typings": "./dist/useTableEditor.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oblosys/react-use-table-editor.git"
  },
  "files": [
    "dist/*",
    "!dist/*.test.*"
  ],
  "scripts": {
    "start": "PUBLIC_URL=/ react-scripts start",
    "eject": "react-scripts eject",
    "build": "tsc -p src/lib",
    "lint-check": "eslint --cache .",
    "lint-fix": "npm run lint-check -- --fix",
    "prepare": "husky install",
    "prettier-check": "prettier -l '**/*.{ts,tsx,json,md,js,scss,yml}'",
    "prettier-fix": "npm run prettier-check -- --write",
    "test": "npm run test-watch -- --watchAll=false",
    "test-watch": "react-scripts test"
  },
  "keywords": [
    "react",
    "table",
    "typed",
    "hooks",
    "editing",
    "dirty-tracking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Oblosys/react-use-table-editor/issues"
  },
  "homepage": "https://github.com/Oblosys/react-use-table-editor#readme",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "25.0.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  }
}
